/* components/Tab/Tab.wxss */
.tabWrap {
  display: flex;
  height: 90rpx;
}
.tabWrap .tab_item {
  flex: 1;
  text-align: center;
  color: #555;
  font-size: 30rpx;
  height: 100%;
  line-height: 90rpx;
}
.tabWrap .active {
  color: var(--themeColor);
  border-bottom: 1rpx solid var(--themeColor);
}
